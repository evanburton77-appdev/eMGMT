<h1>Sign up</h1>

<form action="/insert_agent" method="post">

  <div>
    <label for="email_box">Email</label>
    <input id="email_box" name="query_email" placeholder="Choose a email..." type="text">
  </div>
                 
  <div>
    <label for="first_name_box">First name</label>
      
    <input type="text" id="first_name_box" name="query_first_name">
  </div>
         
  <div>
    <label for="last_name_box">Last name</label>
      
    <input type="text" id="last_name_box" name="query_last_name">
  </div>
         
  <div>
    <label for="photo_box">Photo</label>
      
    <input type="text" id="photo_box" name="query_photo">
  </div>
         
  <div>
    <label for="linkedin_url_box">Linkedin url</label>
      
    <input type="text" id="linkedin_url_box" name="query_linkedin_url">
  </div>
         
  <div>
    <label for="timezone_box">Timezone</label>
      
    <input type="text" id="timezone_box" name="query_timezone">
  </div>
         
  <div>
    <label for="organization_name_box">Organization name</label>
      
    <input type="text" id="organization_name_box" name="query_organization_name">
  </div>
         
  <div>
    <label for="about_box">About</label>
      
    <input type="text" id="about_box" name="query_about">
  </div>
         
  <div>
    <label for="messages_count_box">Messages count</label>
      
    <input type="text" id="messages_count_box" name="query_messages_count">
  </div>
         
  <div>
    <label for="gigs_count_box">Gigs count</label>
      
    <input type="text" id="gigs_count_box" name="query_gigs_count">
  </div>
    
  <div>
    <label for="password_box">Password</label>
    <input id="password_box" name="query_password" placeholder="Choose a password..." type="password">
  </div>

  <div>
    <label for="password_confirmation_box">Password Confirmation</label>
    <input id="password_confirmation_box" name="query_password_confirmation" placeholder="Confirm your password..." type="password">
  </div>

  <button>
    Sign up
  </button>
</form>

<hr>

<p>
  Or, <a href="/agent_sign_in">Sign in</a> instead.
</p>
